config_version=4

[application]

config/name="RootForYou"
run/main_scene="res://scenes/main_scenes/main_game.tscn"
boot_splash/show_image=false
config/icon="res://assets/art/icon.png"

[audio]

default_bus_layout="res://assets/default_bus_layout.tres"

[autoload]

ExitShortcut="*res://scripts/autoloads/exit_shortcut.gd"

[display]

window/size/width=320
window/size/height=180
window/size/resizable=false
window/size/borderless=true
window/size/test_width=640
window/size/test_height=360
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

